{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TeamsAdapter": {
    "MicrosoftAppId": "YOUR_BOT_APP_ID_FROM_AZURE_OR_USER_SECRETS",
    "MicrosoftAppPassword": "YOUR_BOT_APP_PASSWORD_FROM_USER_SECRETS",
    "TenantId": "YOUR_AZURE_TENANT_ID_FROM_USER_SECRETS", 
    "TargetTeamId": "TARGET_TEAM_ID",
    "TargetSharePointLibraryPath": "General" 
  },
  "AIProviders": {
    "Gemini": {
      "ApiKey": "<your-gemini-api-key>",
      "Model": "gemini-1.5-pro-latest"
    },
    "AzureOpenAI": {
      "ApiKey": "<your-azure-openai-key>",
      "Model": "gpt-4o"
    }
  }
}